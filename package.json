{
  "name": "wp-fields",
  "author": "Micah Wood <micah@wpscholar.com>",
  "license": "GPL-2.0+",
  "private": true,
  "scripts": {
    "build": "yarn run build:dev && yarn run build:prod",
    "build:dev": "webpack",
    "build:prod": "cross-env NODE_ENV=production webpack -p",
    "dashboard": "yarn run dashboard:dev",
    "dashboard:dev": "cross-env DASHBOARD=yes webpack-dashboard webpack --watch",
    "dashboard:prod": "cross-env NODE_ENV=production yarn run dashboard:dev -- -p",
    "start": "yarn run dashboard",
    "watch": "yarn run watch:dev",
    "watch:dev": "webpack --watch",
    "watch:prod": "yarn run build:prod -- --watch"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "css-mqpacker": "^6.0.1",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "sass-mq": "^3.3.2",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-dashboard": "^1.0.0-5"
  }
}
